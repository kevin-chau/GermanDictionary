<!-- English-German -->

<!-- B -->
<letter name="B">
	<d:entry id="boy" d:title="boy">
		<d:index d:value="boy"/>
		<div d:priority="2"><h1>boy</h1></div>
		<span class="syntax">
			<span d:pr="US">| boi |</span>
			<span d:pr="US_IPA">| boi |</span>
			<span d:pr="UK_IPA">| boi |</span>
		</span>
		<grammar>noun</grammar>
		<div>
			<ol>
				<li>
					Junge
					<ul>
						<li><example><eng>the boy</eng>: der Junge</example></li>
					</ul>
				</li>
			</ol>
		</div>
	</d:entry>
</letter>




