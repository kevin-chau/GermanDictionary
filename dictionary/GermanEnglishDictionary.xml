<!-- German-English -->
<!-- B -->
<letter name="B">
	<d:entry id="Begrüßungen">
		<d:index d:value="Begrüßungen"/>
		<div>Greetings</div>
	</d:entry>
	
	<d:entry id="Bis bald!">
		<d:index d:value="Bis bald!"/>
		<div>See you soon!</div>
	</d:entry>
	
	<d:entry id="Bis später!"><d:index d:value="Bis später!"/><div>See you later!</div></d:entry>

</letter>

<!-- J -->
<letter name="J">
	<d:entry id="Junge" d:title="Junge">
		<d:index d:value="Junge"/>
		<div d:priority="2"><h1>Junge</h1></div>
		<span class="syntax">
			<span d:pr="US">|  ˈjʊŋə  |</span>
		</span>
		<grammar>masculine noun <b>Pl. </b>Jungen</grammar>
		<div>
			<ol>
				<li>
					Boy
					<ul>
						<li><example><de>der Junge</de>: the boy</example></li>
					</ul>
				</li>
			</ol>
		</div>
	</d:entry>

</letter>

<!-- M -->
<letter name="M">
	<d:entry id="Mädchen" d:title="Mädchen">
		<d:index d:value="Mädchen"/>
		<div d:priority="2"><h1>Mädchen</h1></div>
		<span class="syntax">
			<span d:pr="US">| ˈmɛːtçən |</span>
			<span d:pr="US_IPA">| ˈmɛːtçən |</span>
			<span d:pr="UK_IPA">| ˈmɛːtçən |</span>
		</span>
		<grammar>neuter noun <b>Pl. </b>Mädchen</grammar>
		<div>
			<ol>
				<li>
					Girl
					<ul>
						<li><example><de>ein schönes Mädchen</de>: a beautiful girl</example></li>
					</ul>
				</li>
			</ol>
		</div>
	</d:entry>
</letter>

